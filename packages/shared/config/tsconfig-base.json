{
    "compilerOptions": {
        "target": "es5",
        "allowJs": true,
        "skipLibCheck": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "esModuleInterop": true,
        "module": "CommonJS",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": false,
        "composite": true,
        "importHelpers": true,
    },
    "exclude": ["node_modules", "babel.config.js", "metro.config.js", "jest.config.js"]
}