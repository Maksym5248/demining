{
    "name": "ammo",
    "private": true,
    "version": "1.0.2",
    "build-number": 18,
    "scripts": {
        "android": "react-native run-android --mode=devdebug --appIdSuffix=dev",
        "ios": "react-native run-ios --scheme=\"ammo.dev\" --simulator=\"iPhone 16\"",
        "ios:prod": "react-native run-ios --scheme=\"ammo.prod\" --simulator=\"iPhone 16\"",
        "ios-device": "react-native run-ios --scheme=\"ammo.dev\" --configuration=\"Debug.dev\" --device=\"Max\"",
        "lint": "eslint .",
        "start": "react-native start",
        "test": "jest",
        "clean:js": "/bin/sh ./bin/clean-js.sh",
        "clean:android": "cd android && ./gradlew clean && rm -rf ./.gradle && cd app && rm -rf ./build",
        "apk:dev": "cd android && ./gradlew assembleDevRelease",
        "apk:prod": "cd android && ./gradlew assembleProdRelease",
        "bundle:prod": "cd android && ./gradlew bundleProdRelease",
        "prepare:ios": "yarn install && cd ios && pod install",
        "check:tsc": "tsc --noEmit",
        "check:prettier": "prettier --write",
        "check:eslint": "eslint --fix",
        "update:build-number": "/bin/sh ./bin/update-build-number.sh",
        "clone": "/bin/sh ./bin/clone-base64.sh",
        "prepare:env": "/bin/sh ./bin/prepare-env.sh"
    },
    "dependencies": {
        "@firebase/firestore-types": "^3.0.3",
        "@react-native-community/netinfo": "11.4.1",
        "@react-native-firebase/analytics": "^21.7.1",
        "@react-native-firebase/app": "21.7.1",
        "@react-native-firebase/auth": "21.7.1",
        "@react-native-firebase/firestore": "21.7.1",
        "@react-native-firebase/storage": "21.7.1",
        "@react-navigation/bottom-tabs": "6.6.1",
        "@react-navigation/native": "6.0.6",
        "@react-navigation/native-stack": "6.11.0",
        "@sentry/react-native": "^6.6.0",
        "axios": "1.6.5",
        "dayjs": "1.11.10",
        "formik": "2.4.5",
        "geofire-common": "6.0.0",
        "i18n-js": "4.4.3",
        "lodash": "4.17.21",
        "mobx": "^6.8.0",
        "mobx-react": "9.1.1",
        "mobx-react-lite": "4.0.7",
        "react": "18.3.1",
        "react-native": "0.75.4",
        "react-native-awesome-gallery": "^0.4.3",
        "react-native-config": "1.5.3",
        "react-native-device-info": "^14.0.4",
        "react-native-gesture-handler": "2.21.2",
        "react-native-localize": "3.2.1",
        "react-native-modal": "^13.0.1",
        "react-native-reanimated": "3.15.0",
        "react-native-safe-area-context": "5.1.0",
        "react-native-screens": "4.1.0",
        "react-native-svg": "15.6.0",
        "react-native-svg-transformer": "1.5.0",
        "react-native-theme-mk": "0.1.6",
        "shared-my": "*",
        "shared-my-client": "*",
        "uuid": "3.3.2",
        "yup": "1.2.0"
    },
    "devDependencies": {
        "@babel/core": "^7.20.0",
        "@babel/preset-env": "^7.20.0",
        "@babel/preset-typescript": "^7.23.0",
        "@babel/runtime": "^7.20.0",
        "@react-native/babel-preset": "0.75.4",
        "@react-native/eslint-config": "0.75.4",
        "@react-native/metro-config": "0.75.4",
        "@react-native/typescript-config": "0.75.4",
        "@testing-library/jest-native": "^5.4.3",
        "@testing-library/react-native": "^12.5.3",
        "@types/jest": "^29.5.12",
        "@types/react": "^18.2.6",
        "@types/react-test-renderer": "^18.0.0",
        "babel-jest": "^29.6.3",
        "babel-plugin-module-resolver": "^5.0.2",
        "babel-plugin-transform-remove-console": "^6.9.4",
        "eslint": "8.45.0",
        "eslint-plugin-react-native": "3.11.0",
        "eslint-plugin-testing-library": "5.11.0",
        "husky": "4.3.0",
        "jest": "^29.6.3",
        "lint-staged": "14.0.1",
        "prettier": "2.8.8",
        "react-test-renderer": "18.3.1",
        "typescript": "~5.3.3"
    }
}
